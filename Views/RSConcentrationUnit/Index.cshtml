@model IEnumerable<RSPatients.Models.ConcentrationUnit>

@{
    ViewData["Title"] = "Concentration Unit on file";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List of Concentration Unit</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th> @Html.DisplayNameFor(model => model.ConcentrationCode) </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ConcentrationCode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ConcentrationCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ConcentrationCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ConcentrationCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
